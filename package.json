{
  "name": "uni-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init:prj": "node ./build/init.js --env=development --platform=mp-weixin",
    "open": "node ./build/start",
    "serve": "npm run dev:mp-weixin",
    "build": "npm run build:mp-weixin",
    "dev:h5": "cross-env NODE_ENV=development UNI_PLATFORM=h5 vue-cli-service uni-serve",
    "build:h5": "cross-env NODE_ENV=production UNI_PLATFORM=h5 vue-cli-service uni-build",
    "build:mp-weixin": "cross-env NODE_ENV=production UNI_PLATFORM=mp-weixin vue-cli-service uni-build --minimize",
    "dev:mp-weixin": "cross-env NODE_ENV=development UNI_PLATFORM=mp-weixin vue-cli-service uni-build --watch --minimize",
    "format": "prettier write .",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "start": "concurrently \"npm run dev:mp-weixin\" \"npm run open\"",
    "start2": "run-s dev:mp-weixin open",
    "info": "node node_modules/@dcloudio/vue-cli-plugin-uni/commands/info.js",
    "test:h5": "cross-env UNI_PLATFORM=h5 jest -i",
    "test:mp-weixin": "cross-env UNI_PLATFORM=mp-weixin jest -i"
  },
